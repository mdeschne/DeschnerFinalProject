<h1 class="panel-heading text-center" style="font-size:60px"> <%= current_user.username %>'s Videos </h1>

<% @media.each do |medium| %>

<% if medium.user_id == current_user.id %>

<tr>
  <div class="jumbotron">
    <div class="container text-center">
      <iframe id="player"
       width="640" height="360"
              src="<%= medium.media_url.gsub("watch?v=","embed/")%>"
      ></iframe>

    </div>
<div class="panel-footer text-center"> <a  href="/media/<%= medium.id %>" ><%=medium.title%> </a></div>
<div class="btn-group-justified">
<h3  class="btn btn-primary" style="font-size:20px"> Score:<%= (medium.ups - medium.downs)%></h3>
</div>
</div>
<% else %>
<% end %>
<br>
<% end %>
</div>
